project Huffman is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("compresser.adb", "decompresser.adb", "lca.adb", "main.adb", "test_lca.adb", "test_tree.adb", "tree.adb");

   package Builder is
      for Executable_Suffix use "-gnata";
   end Builder;

   package Compiler is
      for Switches ("ada") use ("-gnata");
   end Compiler;

end Huffman;

